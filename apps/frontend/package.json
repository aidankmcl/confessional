{
  "name": "confessional-frontend",
  "productName": "Confessional",
  "version": "1.0.0",
  "description": "Confessional - A 3D Game Application",
  "main": ".vite/build/main.js",
  "scripts": {
    "dev": "electron-forge start",
    "build": "npm run package:mac:unsigned",
    "start": "npm run dev",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build:backend": "cd ../backend && npm run build",
    "make:all": "npm run build:backend && npm run make",
    "make:mac": "npm run build:backend && electron-forge make --platform=darwin",
    "make:win": "npm run build:backend && electron-forge make --platform=win32",
    "make:linux": "npm run build:backend && electron-forge make --platform=linux",
    "package:all": "node scripts/package.js --all",
    "package:mac": "node scripts/package.js --mac",
    "package:win": "node scripts/package.js --win",
    "package:linux": "node scripts/package.js --linux",
    "package:mac:unsigned": "node scripts/build-unsigned-mac.js"
  },
  "keywords": [],
  "author": {
    "name": "Aidan",
    "email": "aidankmcl@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.4",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "36.2.0",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@dimforge/rapier3d": "^0.16.2",
    "@react-three/drei": "^10.0.7",
    "@react-three/fiber": "^9.1.2",
    "@react-three/rapier": "^2.1.0",
    "@tanstack/react-query": "^5.76.0",
    "@trpc/client": "^11.1.2",
    "@trpc/react-query": "^11.1.2",
    "@trpc/tanstack-react-query": "^11.1.2",
    "@types/three": "^0.176.0",
    "electron-squirrel-startup": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "three": "^0.176.0",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
